package inharitnace;

class Calculator{
	int left, right;
	
	public void setOperends(int left, int right) {
		this.left = left;
		this.right = right;
	}
	
	public void sum() {
		System.out.println(left + right);
	}
	
	public void avtg() {
		System.out.println((left + right)/2);
	}
}
